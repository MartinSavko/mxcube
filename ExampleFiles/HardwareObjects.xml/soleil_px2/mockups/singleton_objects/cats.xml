<equipment class = "SampleChangerMockup">
    <no_of_lids>3</no_of_lids>
    <samples_per_basket>16</samples_per_basket>
    <useUpdateTimer>False</useUpdateTimer>

    <tool>CRYOTONG</tool>
    <pucktype>UNIPUCK</pucktype>

    <!-- SC3 commands and channels -->
